{
  "name": "adonis-kue",
  "version": "5.0.2",
  "description": "Kue provider for the Adonis framework",
  "main": "src/Kue/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test": "ENV_SILENT=true NO_ANSI=false FORCE_COLOR=true nyc node ./bin/index.js --local",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "coverage": "nyc report --reporter=text-lcov"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "author": "Nicholas Rempel <nbrempel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "boxen": "^2.0.0",
    "express-basic-auth": "^1.2.0",
    "kue": "^0.11.6",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.12",
    "@adonisjs/redis": "^2.0.7",
    "@adonisjs/sink": "^1.0.17",
    "child_process": "^1.0.2",
    "coveralls": "^3.0.2",
    "dotenv": "^6.1.0",
    "husky": "^1.1.2",
    "japa": "^2.0.10",
    "japa-cli": "^1.0.1",
    "lint-staged": "^7.3.0",
    "nyc": "^13.1.0",
    "semver": "^5.6.0",
    "standard": "^12.0.1",
    "walk-sync": "^0.3.3"
  },
  "nyc": {
    "include": [
      "src",
      "providers",
      "commands"
    ]
  },
  "standard": {
    "globals": [
      "use",
      "make"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrempel/adonis-kue.git"
  },
  "keywords": [
    "adonis",
    "kue",
    "queue"
  ],
  "bugs": {
    "url": "https://github.com/nrempel/adonis-kue/issues"
  },
  "homepage": "https://github.com/nrempel/adonis-kue#readme"
}
